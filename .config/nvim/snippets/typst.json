{
	"Enum item": {
		"prefix": "ei",
		"body": [
			"#enum.item[",
			"$0",
			"]"
		]
	},
	"List item": {
		"prefix": "li",
		"body": [
			"#list.item[",
			"$0",
			"]"
		]
	},
	"Raw box": {
		"prefix": "r",
		"body": [
			"``` $0 ```"
		]
	},
	"Raw block": {
		"prefix": "rr",
		"body": [
			"```$1",
			"$0",
			"```"
		]
	},
	"Config report": {
		"prefix": "report",
		"body": [
			"#import \"template/report.typ\": *",
			"",
			"#show: doc => conf.with(",
			"  \"$1\",",
			"  lang: \"fr\",",
			")(doc)",
			"$0"
		]
	}
}
