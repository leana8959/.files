			"\\usepackage{tikz}",
			"\\usetikzlibrary{tikzmark}",
			"\\usepackage{tkz-euclide}",
			"\\usepackage{pgfplots}",
			"\\usepackage{caption}",
			"\\pgfplotsset{compat=1.18}",
			"",
