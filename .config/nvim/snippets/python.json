{
	"Dict map": {
		"prefix": "dm",
		"body": [
			"{",
			"\t${1:key}:${2:value}",
			"\tfor ${3:var} in ${4:iterated}",
			"}",
			""
		]
	},
	"List map": {
		"prefix": "lm",
		"body": [
			"[",
			"\t${1:var}${2:transformation}",
			"\tfor ${1:var} in ${3:iterated}",
			"]",
			""
		]
	},
	"For": {
		"prefix": "for",
		"body": [
			"for ${1:var} in ${2:iterable}:",
			"\t"
		]
	},
	"Open, read and save file to variable": {
		"prefix": "open",
		"body": [
			"${1:var} = []",
			"with open(\"${2:file.txt}\") as f:",
			"\t${1:var} = [",
			"\t\tline.strip()",
			"\t\tfor line in f.readlines() if line != '\\n'",
			"\t]",
			""
		]
	}
}
